{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "useUnknownInCatchVariables": false
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js", "dist", "public"]
}
