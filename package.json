{
  "name": "BlixtWallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native run-android --variant chaintestnetDebug --appIdSuffix testnet.debug",
    "start-metro": "react-native start",
    "devtools": "redux-devtools --hostname=192.168.1.100 --port=8000",
    "test": "jest --silent",
    "test-update-snapshot": "jest --silent --updateSnapshot",
    "gen-proto": "pbjs -t static-module -w es6 --force-long -o proto/proto.js proto/rpc.proto && pbts -o proto/proto.d.ts proto/proto.js && pbjs -t static-module -w es6 --force-long -o proto/proto-autopilot.js proto/autopilotrpc/autopilot.proto && pbts -o proto/proto-autopilot.d.ts proto/proto-autopilot.js",
    "testnet-debug": "react-native run-android --variant chaintestnetDebug --appIdSuffix testnet.debug",
    "testnet": "react-native run-android --variant chaintestnetRelease --appIdSuffix testnet",
    "mainnet-debug": "react-native run-android --variant chainmainnetDebug --appIdSuffix debug",
    "mainnet": "react-native run-android --variant chainmainnetRelease",
    "bundle-release": "cd android && ./gradlew clean && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.7.1",
    "@react-native-community/react-native-clipboard": "^1.0.1",
    "base64-js": "1.3.1",
    "bech32": "^1.1.3",
    "bignumber.js": "^9.0.0",
    "buffer": "5.4.3",
    "date-fns": "2.8.1",
    "deep-map-keys": "^2.0.1",
    "easy-peasy": "3.2.6",
    "long": "^4.0.0",
    "native-base": "2.13.8",
    "protobufjs": "^6.8.8",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-animatable": "1.3.3",
    "react-native-barcode-mask": "^1.1.0",
    "react-native-blur-overlay": "1.0.7",
    "react-native-build-config": "^0.3.1",
    "react-native-camera": "3.15.1",
    "react-native-dialogs": "1.1.0",
    "react-native-document-picker": "^3.2.4",
    "react-native-easy-grid": "0.2.2",
    "react-native-fingerprint-scanner": "3.0.2",
    "react-native-fs": "2.16.2",
    "react-native-gesture-handler": "1.5.3",
    "react-native-immersive": "^2.0.0",
    "react-native-keychain": "4.0.5",
    "react-native-linear-gradient": "2.5.6",
    "react-native-push-notification": "^3.1.9",
    "react-native-qrcode-svg": "6.0.1",
    "react-native-reanimated": "1.4.0",
    "react-native-securerandom": "^1.0.0-rc.0",
    "react-native-sqlite-storage": "4.1.0",
    "react-native-svg": "10.0.0",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.10.3",
    "react-navigation-tabs": "2.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/runtime": "7.7.7",
    "@testing-library/react-native": "5.0.3",
    "@types/base64-js": "^1.2.5",
    "@types/bech32": "^1.1.1",
    "@types/color": "3.0.1",
    "@types/jest": "24.0.25",
    "@types/long": "^4.0.0",
    "@types/react": "16.9.17",
    "@types/react-native": "0.60.27",
    "@types/react-native-push-notification": "3.0.7",
    "@types/react-native-sqlite-storage": "3.3.2",
    "@types/react-test-renderer": "16.9.1",
    "@types/remote-redux-devtools": "^0.5.3",
    "babel-jest": "24.9.0",
    "bolt11": "^1.2.6",
    "jest": "24.9.0",
    "jest-date-mock": "^1.0.7",
    "jest-fetch-mock": "3.0.1",
    "jetifier": "1.6.5",
    "metro-react-native-babel-preset": "0.57.0",
    "react-test-renderer": "16.12.0",
    "redux-devtools-cli": "^0.0.1-1",
    "remote-redux-devtools": "^0.5.16",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.1.0",
    "tslint-react-native": "0.0.7",
    "typescript": "3.7.4"
  }
}
