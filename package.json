    "@types/base64-js": "1.3.0",
    "@types/bech32": "1.1.2",
    "@types/color": "3.0.1",
    "@types/crypto-js": "4.0.1",
    "@types/jest": "26.0.15",
    "@types/long": "4.0.1",
    "@types/react": "17.0.0",
    "@types/react-native": "0.63.36",
