    "@types/bech32": "1.1.2",
    "@types/color": "3.0.1",
    "@types/jest": "25.2.1",
    "@types/long": "4.0.1",
    "@types/react": "16.9.35",
    "@types/react-native": "0.62.7",
    "@types/react-native-push-notification": "3.0.9",
    "@types/react-native-sqlite-storage": "3.3.2",
